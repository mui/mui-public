{
  "name": "@mui/internal-bundle-size-checker",
  "version": "1.0.7",
  "description": "Bundle size checker for MUI packages.",
  "type": "module",
  "main": "./src/index.js",
  "types": "./build/index.d.ts",
  "bin": {
    "bundle-size-checker": "./bin/bundle-size-checker.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-public.git",
    "directory": "packages/bundle-size-checker"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "pnpm -w test --project @mui/internal-bundle-size-checker"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./src/index.js"
    },
    "./package.json": "./package.json",
    "./browser": {
      "types": "./build/browser.d.ts",
      "default": "./src/browser.js"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.920.0",
    "@aws-sdk/credential-providers": "^3.920.0",
    "@octokit/rest": "^22.0.0",
    "chalk": "^5.6.2",
    "env-ci": "^11.2.0",
    "execa": "^9.6.0",
    "git-url-parse": "^16.1.0",
    "micromatch": "^4.0.8",
    "piscina": "^5.1.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "vite": "^7.2.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/env-ci": "^3.1.4",
    "@types/micromatch": "^4.0.10",
    "@types/yargs": "^17.0.34"
  }
}
