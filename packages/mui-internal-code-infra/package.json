{
  "name": "@mui/internal-code-infra",
  "version": "0.0.1",
  "description": "Infra scripts and configs to be used across MUI repos.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mui/mui-public.git",
    "directory": "packages/mui-internal-code-infra"
  },
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./prettier": {
      "default": "./src/prettier.mjs"
    },
    "./eslint": {
      "default": "./src/eslint/index.mjs"
    },
    "./eslint-docs": {
      "default": "./src/eslint/docs.mjs"
    }
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.json --noEmit false --emitDeclarationsOnly true",
