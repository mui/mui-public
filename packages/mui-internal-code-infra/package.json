    "@next/eslint-plugin-next": "^15.3.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/estree": "^1.0.8",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.33.1"
  },
  "files": [
    "src",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
