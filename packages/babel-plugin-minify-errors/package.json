{
  "name": "@mui/internal-babel-plugin-minify-errors",
  "version": "2.0.6",
  "author": "MUI Team",
  "description": "This is an internal package not meant for general use.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-public.git",
    "directory": "packages/babel-plugin-minify-errors"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui/material-ui/issues"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/mui-org"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:js": "npm run build:cjs && npm run build:esm",
    "build:cjs": "babel src --out-dir build/cjs --extensions .ts --env-name cjs",
    "build:esm": "babel src --out-dir build/esm --extensions .ts --env-name esm --out-file-extension .mjs",
    "build:types": "tsc -p tsconfig.build.json",
    "prebuild": "rimraf build",
    "typescript": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.27.1",
    "find-package-json": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__helper-module-imports": "^7.18.3",
    "@types/find-package-json": "^1.2.6",
    "babel-plugin-tester": "^12.0.0",
    "rimraf": "^6.0.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@babel/core": "7"
  },
  "sideEffects": false,
  "type": "commonjs",
  "main": "./build/cjs/index.js",
  "exports": {
    ".": {
      "types": "./build/esm/index.d.ts",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.js"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  }
}
