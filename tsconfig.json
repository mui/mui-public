{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "paths": {
      "@mui/internal-docs-infra/*": ["./packages/docs-infra/src/*"],
      "@mui/internal-docs-infra/pipeline/*": ["./packages/docs-infra/src/pipeline/*"],
      "docs/*": ["./docs/*"]
    }
  },
  "references": [
    { "path": "apps/code-infra-dashboard" },
    { "path": "packages/babel-plugin-minify-errors" },
    { "path": "packages/babel-plugin-resolve-imports" },
    { "path": "packages/bundle-size-checker" },
    { "path": "packages/code-infra" },
    { "path": "packages/docs-infra" },
    { "path": "tsconfig.node.json" },
    { "path": "scripts" },
    { "path": "docs" }
  ],
  "files": []
}
